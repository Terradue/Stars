										<xs:element name="dopplerOutsideLimitsFlags" type="xs:boolean"/>
										<xs:element name="dopplerDriftOutsideLimitsFlags" type="xs:boolean"/>
										<xs:element name="chirpQualityLowFlag" type="xs:boolean">
