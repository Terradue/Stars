													<xs:element name="numberOfRejectedBlocks" type="xs:int">
														<xs:annotation>
															<xs:documentation>Doppler centroid estimate rejected (e.g. due to land-sea transition)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="numberOfDopplerRecords" type="xs:int">
														<xs:annotation>
															<xs:documentation>Number of the following azimuth time tagged Doppler data sets</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="dopplerRecordAzimuthSpacing" type="xs:float">
														<xs:annotation>
															<xs:documentation>[s]  nominal value</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="dopplerEstimate" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>polynomials over slant range time</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="timeUTC" type="xs:dateTime">
																	<xs:annotation>
																		<xs:documentation>time tag for all following parameters and polynomials in dopplerCentroidCoordinateType time (zero Doppler)</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="dopplerAtMidRange" type="xs:double">
																	<xs:annotation>
																		<xs:documentation>Value at mid range given for convenience</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="basebandDoppler" type="dblPolynom">
																	<xs:annotation>
																		<xs:documentation>baseband Doppler centroid fit polynomial </xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="geometricDopplerFlag" type="xs:boolean">
																	<xs:annotation>
																		<xs:documentation>geometric absolute Doppler estimate performed and usable</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="geometricDoppler" type="dblPolynom" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>geometric Doppler centroid polynomial</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="dopplerAmbiguity" type="xs:int">
																	<xs:annotation>
																		<xs:documentation>integer multiple of PRF (at mid range)</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="dopplerConsistencyFlag" type="xs:boolean">
																	<xs:annotation>
																		<xs:documentation>unwrapped geometric Doppler estimate consistent with baseband estimate</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="dopplerEstimateConfidence" type="xs:float">
																	<xs:annotation>
																		<xs:documentation>values between 0 and 1 (depends on rejected blocks...)</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="combinedDoppler">
																	<xs:annotation>
																		<xs:documentation>resulting combined Doppler estimate used for processing: baseband estimates with ambiguity from geometry. Pure geometric estimate if baseband confidence value is too low. Identical to baseband records if attitude data not available or quality too bad.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="validityRangeMin" type="xs:double"/>
																			<xs:element name="validityRangeMax" type="xs:double"/>
																			<xs:element name="referencePoint" type="xs:double"/>
																			<xs:element name="polynomialDegree" type="xs:unsignedInt"/>
																			<xs:element name="coefficient" maxOccurs="256">
																				<xs:annotation>
																					<xs:documentation>e.g.: 
a0 * x^0 + a1* x^1 + ... + an*x^n
a0, a1, ..., an = coefficient
0, 1, ..., n = attribute exponent
n= polynomialDegree</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:double">
																							<xs:attribute name="exponent" type="xs:unsignedInt"/>
																						</xs:extension>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="layerIndex" type="xs:int" use="required">
													<xs:annotation>
														<xs:documentation>for each image layer</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="processingParameter" maxOccurs="unbounded">
