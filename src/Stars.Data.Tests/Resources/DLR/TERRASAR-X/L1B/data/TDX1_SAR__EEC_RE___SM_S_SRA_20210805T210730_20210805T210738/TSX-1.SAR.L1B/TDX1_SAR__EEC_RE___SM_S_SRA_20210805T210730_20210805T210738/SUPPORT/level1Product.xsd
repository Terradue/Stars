<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="generalHeader.xsd"/>
	<xs:include schemaLocation="PGS_Types.xsd"/>
	<xs:element name="level1Product">
		<xs:annotation>
			<xs:documentation>This XML file contains the meta data of level 1 processed SAR products</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="generalHeader"/>
				<xs:element name="productComponents">
					<xs:annotation>
						<xs:documentation>Lists and points to the product components. </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="annotation" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>pointer to the annotation file(s). All may point to the same file which includes the respective tags. Further annotation files may be added. </xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="type">
											<xs:annotation>
												<xs:documentation>annotation file type (i.e. this file = MAIN, geolocation grid = GEOREF, ...) which may be supplemented by Geocoding or other annotation files. Size is -1 for type MAIN which points to itself.</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="string255">
													<xs:enumeration value="MAIN"/>
													<xs:enumeration value="GEOREF"/>
													<xs:enumeration value="GEOCODE"/>
													<xs:enumeration value="OTHER"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element ref="file"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="imageData" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Containing one or more polarimetric channels in separate binary data matrices. For DRA mode geometric layers are contained. Data format is 16 bit integer for geocoded products and 16 / 16 bit integer complex for SSCs. ScanSAR SSCs are stored in separate COSAR files for each beam.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="polLayer"/>
										<xs:element name="beamID" type="string20" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Mnemonics for the elevation beams. Beam IDs of each COSAR file for ScanSAR SSC complex product only.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DRAoffset" minOccurs="0">
											<xs:annotation>
												<xs:documentation>SRA, DRAFore, DRAAft: indicates geometric (ATI) layers (not quad pol channels)</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="SRA">
														<xs:annotation>
															<xs:documentation>Single Recieve Antenna (nominal)</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="DRAFore">
														<xs:annotation>
															<xs:documentation>Dual Recieve Antenna</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="DRAAft"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element ref="file"/>
									</xs:sequence>
									<xs:attribute name="layerIndex" type="xs:unsignedInt" use="required">
										<xs:annotation>
											<xs:documentation>number of image layer</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="auxRasterFiles" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>e.g. the Geocoding Mapping Grid, an Incidence Angle Mask (Incidence angle for each image pixel derived from DEM (EEC only)) and/or DEM Mask (Coarse grid enumerating the DEM used for geocoding).</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="type" type="string255">
											<xs:annotation>
												<xs:documentation>e.g. GIM Incidence Angle Mask (EEC products), mapping grid or DEM coverage Mask</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element ref="file"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="quicklooks" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Rescaled images of each image layer readable with common tools for cataloguing and quality control purposes.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="polLayer"/>
										<xs:element name="beamID" type="string20" minOccurs="0">
											<xs:annotation>
												<xs:documentation>beam IDs of each COSAR file for ScanSAR SSC Complex Product only</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DRAoffset" minOccurs="0">
											<xs:annotation>
												<xs:documentation>SRA, DRAFore, DRAAft indicates geometric (ATI) layers (not quad pol)</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="SRA">
														<xs:annotation>
															<xs:documentation>Single Recieve Antenna (nominal)</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="DRAFore">
														<xs:annotation>
															<xs:documentation>Dual Recieve Antenna</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="DRAAft"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element ref="file"/>
									</xs:sequence>
									<xs:attribute name="layerIndex" type="xs:unsignedInt" use="required"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="compositeQuicklook">
								<xs:annotation>
									<xs:documentation>One colour coded and scaled quicklook for all polarimetric channels and the entire scene.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="file"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="browseImage">
								<xs:annotation>
									<xs:documentation>One low resolution quicklook</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="file"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="mapPlot">
								<xs:annotation>
									<xs:documentation>Geographical map showing the image footprint.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="file"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="productInfo">
					<xs:annotation>
						<xs:documentation>General mission, scene and imaging mode related parameters. Basic information on the product content and format. Characterizing the processing mode (e.g. NRT mode).</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="generationInfo">
								<xs:annotation>
									<xs:documentation>key parameters of the product generation and delivery</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="logicalProductID" type="string1024">
											<xs:annotation>
												<xs:documentation>logical ID </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="receivingStation" type="string20">
											<xs:annotation>
												<xs:documentation>e.g. NSG</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="level0ProcessingFacility" type="string20"/>
										<xs:element name="level1ProcessingFacility" type="string20"/>
										<xs:element name="groundOperationsType">
											<xs:annotation>
												<xs:documentation>level 1b user products are only foreseen with entry operational  </xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="operational"/>
													<xs:enumeration value="preoperational"/>
													<xs:enumeration value="instrument"/>
													<xs:enumeration value="test"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="deliveryInfo" type="string1024">
											<xs:annotation>
												<xs:documentation>e.g. NRT processed</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="copyrightInfo" type="string1024">
											<xs:annotation>
												<xs:documentation>general copyright remark</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="qualityInfo">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="qualityInspection">
														<xs:annotation>
															<xs:documentation>TMSP processor internal automatic quality control result: e.g. AUTO_APPROVED or NOT_APPROVED. The latter may result from limit violations which are not necessarily impairing the product quality but are brought to a QA operators attention.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="string255">
																<xs:enumeration value="AUTO_APPROVED"/>
																<xs:enumeration value="OPERATOR_APPROVED"/>
																<xs:enumeration value="NOT_APPROVED"/>
																<xs:enumeration value="UNDEFINED"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="qualityRemark" type="string1024" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="missionInfo">
								<xs:annotation>
									<xs:documentation>mission and orbit parameters at start of scene</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="mission" type="string20">
											<xs:annotation>
												<xs:documentation>TSX-1, TDX-1, ... indicates which instrument acquired this TerraSAR-X mission product.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="orbitPhase" type="xs:int">
											<xs:annotation>
												<xs:documentation>orbit phase 
-1 prelaunch phase
0 launch phase (LEOP?)
1 nominal Orbit
2
...
each phase means an new reference orbit</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="orbitCycle" type="xs:int">
											<xs:annotation>
												<xs:documentation>cycle number </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="absOrbit">
											<xs:annotation>
												<xs:documentation>absolute orbit number at start of scene</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:int">
													<xs:minInclusive value="-1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="relOrbit" type="xs:int">
											<xs:annotation>
												<xs:documentation>relative orbit number</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="numOrbitsInCycle" type="xs:int">
											<xs:annotation>
												<xs:documentation>nominal number of orbits per cycle
depends on phase
currently 167</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="orbitDirection">
											<xs:annotation>
												<xs:documentation>ascending / descending flag</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="ASCENDING"/>
													<xs:enumeration value="DESCENDING"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="acquisitionInfo">
								<xs:annotation>
									<xs:documentation>SAR sensor configuration and instrument modes during acquisition</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="sensor" type="string20">
											<xs:annotation>
												<xs:documentation>sensor identifier: SAR</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element ref="imagingMode"/>
										<xs:element ref="lookDirection"/>
										<xs:element ref="antennaReceiveConfiguration"/>
										<xs:element ref="polarisationMode"/>
										<xs:element name="polarisationList">
											<xs:annotation>
												<xs:documentation>polarization layer list (HH, VV, HV, VH) </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="polLayer" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="elevationBeamConfiguration" type="string20">
											<xs:annotation>
												<xs:documentation>beam idenfication as taken from the order file (e.g. strip_009, scan_009, stripFar_009, ...). For SM, SL: identical with elevation beamID.  In ScanSAR: scan_xyz or wide_xyz with xyz as number of the strip or wideBeam beamID of nearest beam</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="imagingModeSpecificInfo">
											<xs:annotation>
												<xs:documentation>Beam infos for the different modes</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element name="stripMap">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="azimuthBeamID" type="string20">
																	<xs:annotation>
																		<xs:documentation>azimuth beam ID</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="scanSAR">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="numberOfBeams" type="xs:int">
																	<xs:annotation>
																		<xs:documentation>total number for ScanSAR  mode (i.e. 4)</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="beamList">
																	<xs:annotation>
																		<xs:documentation>lists the 4 ScanSAR elevation beams</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="beamID" type="string20" maxOccurs="unbounded"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="azimuthBeamID" type="string20">
																	<xs:annotation>
																		<xs:documentation>azimuth beam ID</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="numberOfBursts" type="xs:int">
																	<xs:annotation>
																		<xs:documentation>ScanSAR</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="burstIndexLast" type="xs:int" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>in COSAR file</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="burstIndexFirst" type="xs:int" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>in COSAR File</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="spotLight">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="numberOfAzimuthBeams" type="xs:int">
																	<xs:annotation>
																		<xs:documentation>total number for spotlight mode</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="azimuthBeamIDFirst" type="string20">
																	<xs:annotation>
																		<xs:documentation>Azimuth beam ID of first azimuth pattern used.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="azimuthBeamIDLast" type="string20"/>
																<xs:element name="azimuthSteeringAngleFirst" type="xs:float">
																	<xs:annotation>
																		<xs:documentation>nominal azimuth steering angle of first azimuth beam referenced to mechanical boresight (not the attitude depending pointing). From IOCS Aux Product antenna pattern information in  degree.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="azimuthSteeringAngleLast" type="xs:float">
																	<xs:annotation>
																		<xs:documentation>[degree]</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="productVariantInfo">
								<xs:annotation>
									<xs:documentation>product variant description</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="productType" type="string128">
											<xs:annotation>
												<xs:documentation>mnemonic e.g. MGD_SE_SM_S</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="productVariant">
											<xs:annotation>
												<xs:documentation>SSC | MGD | GEC | EEC</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="SSC"/>
													<xs:enumeration value="MGD"/>
													<xs:enumeration value="GEC"/>
													<xs:enumeration value="EEC"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="projection">
											<xs:annotation>
												<xs:documentation>slant range, ground range, map</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="SLANTRANGE"/>
													<xs:enumeration value="UNDEFINED"/>
													<xs:enumeration value="GROUNDRANGE"/>
													<xs:enumeration value="MAP"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="mapProjection" minOccurs="0">
											<xs:annotation>
												<xs:documentation>for geocoding only: UTM, UPS</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="UTM"/>
													<xs:enumeration value="UPS"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="resolutionVariant">
											<xs:annotation>
												<xs:documentation>SE, RE</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="SE"/>
													<xs:enumeration value="RE"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="radiometricCorrection">
											<xs:annotation>
												<xs:documentation>absolutely calibrated, relative calibrated, not calibrated, ...</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="string128">
													<xs:enumeration value="CALIBRATED"/>
													<xs:enumeration value="NOTCALIBRATED"/>
													<xs:enumeration value="UNDEFINED"/>
													<xs:enumeration value="RELCALIBRATED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="imageDataInfo">
								<xs:annotation>
									<xs:documentation>image layer format</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="pixelValueID" type="string128">
											<xs:annotation>
												<xs:documentation>complex amplitude and phase, radar brightness (beta nought), sigma nought, ...</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="imageDataType">
											<xs:annotation>
												<xs:documentation>detected or complex</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="COMPLEX"/>
													<xs:enumeration value="DETECTED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="imageDataFormat">
											<xs:annotation>
												<xs:documentation>GeoTIFF for geocoded images, COSAR for SSC products</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="string255">
													<xs:enumeration value="GEOTIFF"/>
													<xs:enumeration value="COSAR"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="numberOfLayers" type="xs:int">
											<xs:annotation>
												<xs:documentation>number of polarizations + DRA channels + elevation beams (ScanSAR)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="imageDataDepth" type="xs:int">
											<xs:annotation>
												<xs:documentation>bits per pixel (16bit detected or 2x16bit complex)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="imageStorageOrder">
											<xs:annotation>
												<xs:documentation>e.g. row by row</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="ROWBYROW"/>
													<xs:enumeration value="COLBYCOL"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="rowContent" type="string20">
											<xs:annotation>
												<xs:documentation>e.g. rangelines,  northing (for geocoded products), x, ...</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="columnContent" type="string20">
											<xs:annotation>
												<xs:documentation>e.g. azimuth, y</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="imageRaster" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Information that might differ for ScanSAR SSC beams but is the same for all polarisations</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="numberOfRows" type="xs:int"/>
													<xs:element name="numberOfColumns" type="xs:int"/>
													<xs:element name="rowSpacing">
														<xs:annotation>
															<xs:documentation>spacing of samples within a row from common raster [s  or m]. Signs give orientation. E.g. range sampling.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:float">
																	<xs:attribute name="units">
																		<xs:annotation>
																			<xs:documentation>seconds [s] for SSCs, meter [m] for projected images.</xs:documentation>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:NMTOKENS">
																				<xs:enumeration value="s"/>
																				<xs:enumeration value="m"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="columnSpacing">
														<xs:annotation>
															<xs:documentation>spacing within a column. E.g. azimuth sampling</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:float">
																	<xs:attribute name="units">
																		<xs:annotation>
																			<xs:documentation>seconds [s] for SSCs, meter [m] for projected images.</xs:documentation>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:NMTOKENS">
																				<xs:enumeration value="s"/>
																				<xs:enumeration value="m"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="groundRangeResolution" type="xs:double">
														<xs:annotation>
															<xs:documentation>nominel worst case (at near range)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="azimuthResolution" type="xs:double">
														<xs:annotation>
															<xs:documentation>nominal</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="azimuthLooks" type="xs:float">
														<xs:annotation>
															<xs:documentation>effective number of looks (ENL)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="rangeLooks" type="xs:float"/>
												</xs:sequence>
												<xs:attribute name="beamID" type="string20">
													<xs:annotation>
														<xs:documentation>for ScanSAR SSCs</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="sceneInfo">
								<xs:annotation>
									<xs:documentation>time and scene location information</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="sceneID" type="string1024">
											<xs:annotation>
												<xs:documentation>orbit + timing information</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="start">
											<xs:annotation>
												<xs:documentation>time stamps of first  image row (all processed azimuth times should be Doppler zero times (see imageCoordinateType))</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="timeUTC" type="xs:dateTime">
														<xs:annotation>
															<xs:documentation>e.g. 2009-03-09T20:29:17.722418Z in CCSDS ASCII time format</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="timeGPS" type="xs:long">
														<xs:annotation>
															<xs:documentation>integer GPS seconds</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="timeGPSFraction" type="xs:float">
														<xs:annotation>
															<xs:documentation>fraction of GPS seconds</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="stop">
											<xs:annotation>
												<xs:documentation>time stamps of  last image row</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="timeUTC" type="xs:dateTime"/>
													<xs:element name="timeGPS" type="xs:long"/>
													<xs:element name="timeGPSFraction" type="xs:float"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="rangeTime">
											<xs:annotation>
												<xs:documentation>minimum and maximum of the two way slant range times over the entire (e.g. ScanSAR) scene.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="firstPixel" type="xs:double">
														<xs:annotation>
															<xs:documentation>[s] minimum range time of entire scene</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="lastPixel" type="xs:double">
														<xs:annotation>
															<xs:documentation>[s] maximum rang time of entire scene</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="sceneAzimuthExtent" type="xs:double">
											<xs:annotation>
												<xs:documentation>(approximatly and at mid-range) in meter</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="sceneRangeExtent" type="xs:double" minOccurs="0"/>
										<xs:element name="sceneCenterCoord">
											<xs:annotation>
												<xs:documentation>geographic coordinates</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="refRow" type="xs:int" minOccurs="0">
														<xs:annotation>
															<xs:documentation>position in image row. range sample position for SSCs. Annotated image sample positions for complex products have only informative purposes. All localisation is based on timing information. </xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="refColumn" type="xs:int" minOccurs="0">
														<xs:annotation>
															<xs:documentation>position in image column. Deskewed azimuth position (ASRI+index-1) for SSCs in COSAR format.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="lat" type="latitudeDegType">
														<xs:annotation>
															<xs:documentation>geographical latitude positive towards north</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="lon" type="longitudeDegType">
														<xs:annotation>
															<xs:documentation>geographical longitude positive towards east</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="azimuthTimeUTC" type="xs:dateTime">
														<xs:annotation>
															<xs:documentation>geo coordinates are derived from this timinig information using the corrections annotated in the geo reference annotation component.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="rangeTime" type="xs:double">
														<xs:annotation>
															<xs:documentation>geo coordinates are derived from this timinig information using the corrections annotated in the geo reference annotation component.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="incidenceAngle" type="xs:double">
														<xs:annotation>
															<xs:documentation>incidence angle</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="sceneAverageHeight" type="xs:double">
											<xs:annotation>
												<xs:documentation>meter (w.r.t. to reference frame)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="sceneCornerCoord" minOccurs="4" maxOccurs="4">
											<xs:annotation>
												<xs:documentation>geographic corner coords of the illuminated scene. Even for complex products are these not necessarily the image frame coordinates (due to echo window position shifts, ...). The image frame coordinates of geocoded products are given in the geocodedImageInfo section.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="refRow" type="xs:int" minOccurs="0">
														<xs:annotation>
															<xs:documentation>position in image row. range sample position for SSCs.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="refColumn" type="xs:int" minOccurs="0">
														<xs:annotation>
															<xs:documentation>position in image column. Deskewed azimuth position for SSCs.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="lat" type="latitudeDegType">
														<xs:annotation>
															<xs:documentation>geographical latitude positive towards north</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="lon" type="longitudeDegType">
														<xs:annotation>
															<xs:documentation>geographical longitude, positive towards east</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="azimuthTimeUTC" type="xs:dateTime">
														<xs:annotation>
															<xs:documentation>geo coordinates are derived from this timinig information using the corrections annotated in the geo reference annotation component.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="rangeTime" type="xs:double">
														<xs:annotation>
															<xs:documentation>geo coordinates are derived from this timinig information using the corrections annotated in the geo reference annotation component.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="incidenceAngle" type="xs:double">
														<xs:annotation>
															<xs:documentation>incidence angle</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="sceneLocalisationAccuracy" minOccurs="0">
											<xs:annotation>
												<xs:documentation>error in product placement as difference between ordered and achieved scene center center coordinates (Spotlight mode only)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="referenceProjection" type="string128">
														<xs:annotation>
															<xs:documentation>image projection for the error calculation (ground range, WGS84)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="latError" type="latitudeDegType">
														<xs:annotation>
															<xs:documentation>geographical latitude positive towards north</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="lonError" type="longitudeDegType">
														<xs:annotation>
															<xs:documentation>geographical longitude positive towards east</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="headingAngle" type="xs:float">
											<xs:annotation>
												<xs:documentation>rotation of azimuth heading clock wise w.r.t. North at mid-scene in degree</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="previewInfo">
								<xs:annotation>
									<xs:documentation>Quicklook information</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="quicklooks">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="imageDataFormat" type="string255">
														<xs:annotation>
															<xs:documentation>TIFF greyscale</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataDepth" type="xs:int">
														<xs:annotation>
															<xs:documentation>16bit</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataScaling" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Scaling and Gammma correction of QLs may be applied and can be different for each polarisation and beam.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="polLayer" minOccurs="0"/>
																<xs:element name="beamID" type="string20" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>mnemonics for the beams. Beam IDs of each beam file for ScanSAR SSC complex product only</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DRAoffset" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>SRA, DRAFore, DRAAft: indicates geometric (ATI) layers (not quad pol channels)</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:NMTOKENS">
																			<xs:enumeration value="SRA">
																				<xs:annotation>
																					<xs:documentation>Single Recieve Antenna (nominal)</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="DRAFore">
																				<xs:annotation>
																					<xs:documentation>Dual Recieve Antenna</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="DRAAft"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="gamma" type="xs:double"/>
																<xs:element name="scalingFactor" type="xs:double"/>
																<xs:element name="clipUpper" type="xs:double"/>
																<xs:element name="clipLower" type="xs:double"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="imageRaster" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Size and spacing might differ for quicklooks of ScanSAR SSC beams but is the same for all polarisations. </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="numberOfRows" type="xs:int"/>
																<xs:element name="numberOfColumns" type="xs:int"/>
																<xs:element name="columnBlockLength" type="xs:float">
																	<xs:annotation>
																		<xs:documentation>resampling factor (in pixel)</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="rowBlockLength" type="xs:float"/>
																<xs:element name="rowSpacing">
																	<xs:annotation>
																		<xs:documentation>signs give orientation</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:float">
																				<xs:attribute name="units">
																					<xs:annotation>
																						<xs:documentation>seconds [s] for SSCs, meter [m] for projected images.</xs:documentation>
																					</xs:annotation>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
																<xs:element name="columnSpacing">
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:float">
																				<xs:attribute name="units">
																					<xs:annotation>
																						<xs:documentation>seconds [s] for SSCs, meter [m] for projected images.</xs:documentation>
																					</xs:annotation>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
																<xs:element name="projectedSpacingAzimuth" type="xs:float" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>nominal projected pixel spacing on ground [m] for SSCs</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="projectedSpacingRange" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="groundNear" type="xs:float"/>
																			<xs:element name="groundFar" type="xs:float"/>
																			<xs:element name="slantRange" type="xs:float"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="beamID" type="string20"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="compositeQuicklook">
											<xs:annotation>
												<xs:documentation>Colour coded image for dual, twin or quad pol data. For single pol data it is a rescaled QL of the entire scene.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="imageDataFormat" type="string255">
														<xs:annotation>
															<xs:documentation>TIFF RGB</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataDepth" type="xs:int">
														<xs:annotation>
															<xs:documentation>24 bit</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="polLayerCode" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>which polarisation information is coded with which colour</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="polarisation" type="string80">
																	<xs:annotation>
																		<xs:documentation>e.g. HH or HH-VV, ...</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="colour" type="string20"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="browseImage">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="imageDataFormat" type="string255">
														<xs:annotation>
															<xs:documentation>e.g. TIFF or JPEG</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataDepth" type="xs:int">
														<xs:annotation>
															<xs:documentation>24bit</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="mapPlotFormat" type="string255">
											<xs:annotation>
												<xs:documentation>e.g. PNG, GIF</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="productSpecific">
					<xs:annotation>
						<xs:documentation>Specific information for SSCs, detected and geocoded products.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="complexImageInfo" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>additional information for SSCs that is also given for further processed products</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="commonPRF" type="xs:double">
											<xs:annotation>
												<xs:documentation>[Hz] intermediate "output PRF"  used during processing (not identical to the final spacing for detected products)</xs:documentation>
											</xs:annotation>
										</xs:element>
