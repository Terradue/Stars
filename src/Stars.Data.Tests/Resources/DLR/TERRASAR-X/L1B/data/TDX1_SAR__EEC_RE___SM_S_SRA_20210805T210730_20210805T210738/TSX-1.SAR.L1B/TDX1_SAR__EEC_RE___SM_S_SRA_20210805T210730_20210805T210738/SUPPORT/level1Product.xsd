															<xs:documentation>azimuth and range time (t and tau)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataFormat" type="string255">
														<xs:annotation>
															<xs:documentation>plain binary</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataDepth" type="xs:int">
														<xs:annotation>
															<xs:documentation>bits per pixel = 2*double</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageStorageOrder">
														<xs:annotation>
															<xs:documentation>row by row</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:NMTOKENS">
																<xs:enumeration value="ROWBYROW"/>
																<xs:enumeration value="COLBYCOL"/>
																<xs:enumeration value="UNDEFINED"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="gridReferenceTime">
														<xs:annotation>
															<xs:documentation>origin of time coordinate system</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="tReferenceTimeUTC" type="xs:dateTime">
																	<xs:annotation>
																		<xs:documentation>for the t time values in the grid. E.g. the frame azimuth start time.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="tauReferenceTime" type="xs:double">
																	<xs:annotation>
																		<xs:documentation>for the tau range time values in the grid</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="refRow" type="xs:long">
																	<xs:annotation>
																		<xs:documentation>reference pixel position in an image file row. E.g. 1</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="refCol" type="xs:long">
																	<xs:annotation>
																		<xs:documentation>reference pixel position in image column. E.g. 1</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="imageRaster">
														<xs:annotation>
															<xs:documentation>one for all polarisations. </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="numberOfRows" type="xs:int"/>
																<xs:element name="numberOfColumns" type="xs:int"/>
																<xs:element name="rowSpacing" type="xs:float">
																	<xs:annotation>
																		<xs:documentation>in pixel of the geocoded image raster. Signs give orientation</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="columnSpacing" type="xs:float"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="incidenceAngleMaskDescription" minOccurs="0">
											<xs:annotation>
												<xs:documentation>GIM format (for EECs only).</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="pixelValueID" type="string128">
														<xs:annotation>
															<xs:documentation>local incidence angle and shadow-layover flags</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataFormat" type="string255">
														<xs:annotation>
															<xs:documentation>GeoTIFF</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataDepth" type="xs:int">
														<xs:annotation>
															<xs:documentation>16bits per pixel</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageRaster" minOccurs="0">
														<xs:annotation>
															<xs:documentation>one for all polarisations.  Has the same dimensions as the geocoded image.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="numberOfRows" type="xs:int"/>
																<xs:element name="numberOfColumns" type="xs:int"/>
																<xs:element name="rowSpacing" type="xs:float">
																	<xs:annotation>
																		<xs:documentation>signs give orientation</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="columnSpacing" type="xs:float"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="DEMCoverageMapDescription" minOccurs="0">
											<xs:annotation>
												<xs:documentation>indicates the DEM source for each image pixel (for EECs only).</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="pixelValueID" type="string128">
														<xs:annotation>
															<xs:documentation>DEM sources codes</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataFormat" type="string255">
														<xs:annotation>
															<xs:documentation>GeoTIFF</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageDataDepth" type="xs:int">
														<xs:annotation>
															<xs:documentation>16 bit</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="imageRaster" minOccurs="0">
														<xs:annotation>
															<xs:documentation>one for all polarisations.  Has the same dimensions as the geocoded image.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="numberOfRows" type="xs:int"/>
																<xs:element name="numberOfColumns" type="xs:int"/>
																<xs:element name="rowSpacing" type="xs:float">
																	<xs:annotation>
																		<xs:documentation>signs give orientation</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="columnSpacing" type="xs:float"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="setup">
					<xs:annotation>
						<xs:documentation>Screening and processing chain setup and control parameters, input data and relevant software used.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="orderInfo">
								<xs:annotation>
									<xs:documentation>Order requests to TMSP leading to the final product</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="orderType" type="string128">
											<xs:annotation>
												<xs:documentation>L1B-SAR</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="processingPriority" type="string20">
											<xs:annotation>
												<xs:documentation>NRT</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="orbitAccuracy">
											<xs:annotation>
												<xs:documentation>REFE for reference
PRED for predicted
QUKL for quicklook
RAPD for rapid
SCIE for science</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKEN">
													<xs:enumeration value="REFE"/>
													<xs:enumeration value="PRED"/>
													<xs:enumeration value="QUKL"/>
													<xs:enumeration value="RAPD"/>
													<xs:enumeration value="SCIE"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="sceneSpecification">
											<xs:annotation>
												<xs:documentation>either start + stop time or scene center coordinates (spotlight only)</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="TIME"/>
													<xs:enumeration value="CENTERCOORDS"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="orderedScene">
											<xs:complexType>
												<xs:choice>
													<xs:element name="sceneCenterCoord">
														<xs:annotation>
															<xs:documentation>spotlight mode only. Given for localisation accuracy assessment only since one datatake corresponds to one scene.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="lat" type="latitudeDegType">
																	<xs:annotation>
																		<xs:documentation>geographical latitude , positive towards north</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="lon" type="longitudeDegType">
																	<xs:annotation>
																		<xs:documentation>geographical longitude, positive towards east</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="centerCoordHeight" type="xs:double" minOccurs="0"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="sceneExtent">
														<xs:annotation>
															<xs:documentation>zero Doppler times</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="startTimeUTC" type="xs:dateTime"/>
																<xs:element name="stopTimeUTC" type="xs:dateTime"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element ref="imagingMode"/>
										<xs:element ref="lookDirection"/>
										<xs:element ref="antennaReceiveConfiguration"/>
										<xs:element ref="polarisationMode"/>
										<xs:element name="polList">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="polLayer" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="elevationBeamConfiguration" type="string20">
											<xs:annotation>
												<xs:documentation>Beam identification as taken from the order file.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="productVariant">
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="SSC"/>
													<xs:enumeration value="MGD"/>
													<xs:enumeration value="GEC"/>
													<xs:enumeration value="EEC"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="resolutionVariant">
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="SE"/>
													<xs:enumeration value="RE"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="projection">
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="GROUNDRANGE"/>
													<xs:enumeration value="SLANTRANGE"/>
													<xs:enumeration value="UNDEFINED"/>
													<xs:enumeration value="MAP"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="geocodingOrder" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="mapProjection">
														<xs:annotation>
															<xs:documentation>geocoded products: AUTO, UTM or UPS projection prefered for transition zones</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:NMTOKENS">
																<xs:enumeration value="UTM"/>
																<xs:enumeration value="UPS"/>
																<xs:enumeration value="UNDEFINED"/>
																<xs:enumeration value="AUTO"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="countryID" type="string128">
														<xs:annotation>
															<xs:documentation>for geocoding</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="datumID" type="string128">
														<xs:annotation>
															<xs:documentation>for geocoding</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="userData" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="customerOrderID" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>acquisitionOrderID</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="customerOrderName" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>aka assocName</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="customerOrderItemID" type="xs:string" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="procGainAttenuation" type="xs:double" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="inputData">
								<xs:annotation>
									<xs:documentation>Products identifieres (logical IDs), generation times and versions of data used for L1B Processing</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="logicalDataTakeID" type="string1024">
											<xs:annotation>
												<xs:documentation>logical datatake ID built by receiving station, datatake file number, commanded acquisition start time</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="uniqueDataTakeID" type="string1024">
											<xs:annotation>
												<xs:documentation>unique number of datatake generated by MOS</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="level0ProductID" type="string1024"/>
										<xs:element name="L0BaseGenerationTimeUTC" type="xs:dateTime"/>
										<xs:element name="L0SARGenerationTimeUTC" type="xs:dateTime"/>
										<xs:element name="IOCSAuxProductID" type="string1024"/>
										<xs:element name="IOCSAuxProductFileName" type="string1024"/>
										<xs:element name="IOCSAuxProductVersion" type="string80"/>
										<xs:element name="IOCSAuxProductGenerationTimeUTC" type="xs:dateTime"/>
										<xs:element name="orbitProductID" type="string1024"/>
										<xs:element name="orbitProductFileName" type="string1024"/>
										<xs:element name="orbitAccuracy">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="20"/>
													<xs:enumeration value="REFE"/>
													<xs:enumeration value="PRED"/>
													<xs:enumeration value="QUKL"/>
													<xs:enumeration value="RAPD"/>
													<xs:enumeration value="SCIE"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="orbitProductGenerationTimeUTC" type="xs:dateTime"/>
										<xs:element name="attitudeProductID" type="string1024" minOccurs="0"/>
										<xs:element name="attitudeProductFileName" type="string1024" minOccurs="0"/>
										<xs:element name="attitudeProductGenerationTimeUTC" type="xs:dateTime" minOccurs="0"/>
										<xs:element name="configurationID" type="string1024">
											<xs:annotation>
												<xs:documentation>processor and product configuration file</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="configurationFileName" type="string1024">
											<xs:annotation>
												<xs:documentation>processor and product configuration file</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="configurationGenerationTimeUTC" type="xs:dateTime"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="processingSteps">
								<xs:annotation>
									<xs:documentation>history of software components used to generate this product</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="software" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>lists each major component of the processing chain </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="softwareID" type="string128">
														<xs:annotation>
															<xs:documentation>Base Screener,... Geocoding, Formatter, ...</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="softwareVersion" type="string128"/>
													<xs:element name="algorithm" type="string255" minOccurs="0">
														<xs:annotation>
															<xs:documentation>main algorithm; e.g. Extended Chirp Scaling Algorithm for SL ECSS,...</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="processing">
					<xs:annotation>
						<xs:documentation>Processor and product configuration parameters and parameters of the data determined during screening and processing.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="signalDataAnalysis" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Datatake structure, Cal+Noise pulse and Raw Data Analysis and Correction results. Based on L0 annotation.  Some or all of these informations may be included (e.g. for processing of erroneous datatakes, experimental modes or specific IOCS/SEC requirements).</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="signalDataCoordinateType">
											<xs:annotation>
												<xs:documentation>raw data time tags (not zero Doppler)</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="string20">
													<xs:enumeration value="RAW"/>
													<xs:enumeration value="ZERODOPPLER"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="dataGapInfo" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="numberOfGaps" type="xs:int">
														<xs:annotation>
															<xs:documentation>one or more consecutive missing ISPs</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="dataGap" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="segmentInfo" type="segmentInfoType"/>
																<xs:element name="numberOfMissingISPs" type="xs:int"/>
																<xs:element name="ISPdesignator" type="string20" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>missing, corrupted, ...</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="rawDataInfo" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="numberOfRawDataBlocks" type="xs:int">
														<xs:annotation>
															<xs:documentation>small blocks of data in range and azimuth used for estimate and correction</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="rawDataStatisticsAndCorrection" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>I, Q Channel and ADC statistic results for blocks of each raw data channel</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="blockInfo">
																	<xs:annotation>
																		<xs:documentation>unlike the segments, the blocks are also split in range</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="instrumentChannel" type="xs:string">
																				<xs:annotation>
																					<xs:documentation>in QUAD mode these are 4 Sum/Dif channels and do NOT correspond to image layers</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="beamID" type="string20" minOccurs="0"/>
																			<xs:element name="dataBlock">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="startTimeUTC" type="xs:dateTime">
																							<xs:annotation>
																								<xs:documentation>Mid-range azimuth time (in the signal data coord system)</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="stopTimeUTC" type="xs:dateTime"/>
																						<xs:element name="rangeTimeFirst" type="xs:double"/>
																						<xs:element name="rangeTimeLast" type="xs:double"/>
																						<xs:element name="numberOfLines" type="xs:int"/>
																						<xs:element name="numberOfSamples" type="xs:int"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																		<xs:attribute name="blockID" type="xs:int" use="required">
																			<xs:annotation>
																				<xs:documentation>number</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
																<xs:element name="IQbias">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="IChannel" type="xs:float"/>
																			<xs:element name="QChannel" type="xs:float"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="IQstdDeviation">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="IChannel" type="xs:float"/>
																			<xs:element name="QChannel" type="xs:float"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="IQGainImbalance" type="xs:float"/>
																<xs:element name="IQNonOrthogonality" type="xs:float"/>
																<xs:element name="totalNumberOfValidSamples">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="IChannel" type="xs:long"/>
																			<xs:element name="QChannel" type="xs:long"/>
																		</xs:sequence>
