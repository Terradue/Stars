											</xs:annotation>
										</xs:element>
										<xs:element name="dopplerGeometricEstimationMethod" type="string255">
											<xs:annotation>
												<xs:documentation>orbit and attitude data evaluation</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="dopplerCentroidCoordinateType">
											<xs:annotation>
												<xs:documentation>image data time tags (zero Doppler)</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="string20">
													<xs:enumeration value="RAW"/>
													<xs:enumeration value="ZERODOPPLER"/>
													<xs:enumeration value="UNDEFINED"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="dopplerCentroid" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>f_DC Estimates for each layer and ScanSAR beam and derived processing parameters</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="polLayer"/>
													<xs:element name="DRAoffset" minOccurs="0">
														<xs:annotation>
															<xs:documentation>SRA, DRAFore, DRAAft</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:NMTOKENS">
																<xs:enumeration value="SRA">
																	<xs:annotation>
																		<xs:documentation>Single Recieve Antenna (nominal)</xs:documentation>
																	</xs:annotation>
																</xs:enumeration>
																<xs:enumeration value="DRAFore">
																	<xs:annotation>
																		<xs:documentation>Dual Recieve Antenna</xs:documentation>
																	</xs:annotation>
																</xs:enumeration>
																<xs:enumeration value="DRAAft"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="beamID" type="string20" minOccurs="0"/>
