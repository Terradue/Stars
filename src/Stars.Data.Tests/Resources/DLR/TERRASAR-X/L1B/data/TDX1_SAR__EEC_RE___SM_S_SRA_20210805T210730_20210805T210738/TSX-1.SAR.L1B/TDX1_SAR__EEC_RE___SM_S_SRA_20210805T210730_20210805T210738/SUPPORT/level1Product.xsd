																		<xs:documentation>[ns]</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="internalDelay" maxOccurs="4">
																	<xs:annotation>
																		<xs:documentation>[ns] instrument electronic time delay</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:float">
																				<xs:attribute name="polarisationChannel" use="required">
																					<xs:annotation>
																						<xs:documentation>HH, VV. HV, VH</xs:documentation>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="xs:NMTOKEN">
																							<xs:enumeration value="HH"/>
																							<xs:enumeration value="VV"/>
																							<xs:enumeration value="HV"/>
																							<xs:enumeration value="VH"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="ADCandIQCharacteristics" minOccurs="0">
														<xs:annotation>
															<xs:documentation>segment present only if corresponding processing/signalDataAnalysis available.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="I-Q-Characteristics" minOccurs="2" maxOccurs="2">
																	<xs:annotation>
																		<xs:documentation>start values by OGC</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="bias" type="xs:double"/>
																			<xs:element name="imbalance" type="xs:double"/>
																			<xs:element name="non-orthogonality" type="xs:double"/>
																		</xs:sequence>
																		<xs:attribute name="channel" use="required">
																			<xs:simpleType>
																				<xs:restriction base="xs:NMTOKEN">
																					<xs:enumeration value="I"/>
																					<xs:enumeration value="Q"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
