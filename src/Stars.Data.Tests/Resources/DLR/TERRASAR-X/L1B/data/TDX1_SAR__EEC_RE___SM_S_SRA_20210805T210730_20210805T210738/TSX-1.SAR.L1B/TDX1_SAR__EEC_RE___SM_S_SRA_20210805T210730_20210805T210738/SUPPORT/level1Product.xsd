															<xs:sequence>
																<xs:element name="dopplerMidRangeFirst" type="xs:double"/>
																<xs:element name="dopplerMidRangeLast" type="xs:double"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="polLayerDopplerOffset" type="xs:double" minOccurs="0">
														<xs:annotation>
															<xs:documentation>f_dc offset of this polarisation layer to the mean of all polarisations (mid-scene).</xs:documentation>
														</xs:annotation>
