											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="absCalFactor" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>[dB]
original absolute calibration factor without processor scaling</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:float">
																	<xs:attribute name="imagingMode" use="required"/>
																	<xs:attribute name="beamID" type="string20" use="required">
																		<xs:annotation>
																			<xs:documentation>strip_007, spot_008</xs:documentation>
																		</xs:annotation>
																	</xs:attribute>
																	<xs:attribute name="polarisationChannel" use="required">
																		<xs:annotation>
																			<xs:documentation>HH, VV, HV, VH</xs:documentation>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:NMTOKENS">
																				<xs:enumeration value="HH"/>
																				<xs:enumeration value="VV"/>
																				<xs:enumeration value="HV"/>
																				<xs:enumeration value="VH"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																	<xs:attribute name="lookDirection" use="required">
																		<xs:simpleType>
																			<xs:restriction base="xs:NMTOKENS">
																				<xs:enumeration value="left"/>
																				<xs:enumeration value="right"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																	<xs:attribute name="antennaMode" use="required">
																		<xs:annotation>
																			<xs:documentation>SRA, DRAFore, DRAAft</xs:documentation>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:NMTOKENS">
																				<xs:enumeration value="SRA">
																					<xs:annotation>
																						<xs:documentation>Single Recieve Antenna (nominal)</xs:documentation>
																					</xs:annotation>
																				</xs:enumeration>
																				<xs:enumeration value="DRAFore">
																					<xs:annotation>
																						<xs:documentation>Dual Recieve Antenna</xs:documentation>
																					</xs:annotation>
																				</xs:enumeration>
																				<xs:enumeration value="DRAAft"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="totalInstrumentTimeDelay">
														<xs:annotation>
