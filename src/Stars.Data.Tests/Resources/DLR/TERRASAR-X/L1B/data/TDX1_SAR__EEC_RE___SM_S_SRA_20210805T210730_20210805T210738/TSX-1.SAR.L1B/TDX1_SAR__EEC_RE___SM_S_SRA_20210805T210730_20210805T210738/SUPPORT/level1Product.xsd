																	</xs:complexType>
																</xs:element>
																<xs:element name="QChannel">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="QAdcOvlMaxLimit" type="xs:float">
																				<xs:annotation>
																					<xs:documentation>limit of relative Q-Data Overflow in percentage, maximum over datatake</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="QAdcOvlStdLimit" type="xs:float">
