														<xs:complexType>
															<xs:sequence>
																<xs:element name="referenceChirp" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>waveform generation parameters</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="pulseType" type="string80">
																				<xs:annotation>
																					<xs:documentation>custom waveform, ...</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="chirpDesignator">
																				<xs:annotation>
																					<xs:documentation>nominal chirp, replica, ideal chirp, _</xs:documentation>
																				</xs:annotation>
																				<xs:simpleType>
																					<xs:restriction base="xs:NMTOKENS">
																						<xs:enumeration value="IDEAL"/>
																						<xs:enumeration value="NOMINAL"/>
																						<xs:enumeration value="REPLICA"/>
																						<xs:enumeration value="MODEL"/>
																						<xs:enumeration value="CUSTOM"/>
																						<xs:enumeration value="UNDEFINED"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element ref="chirpSlope"/>
																			<xs:element name="pulseLength" type="xs:double">
																				<xs:annotation>
																					<xs:documentation>instrument code! Pulse length in seconds=value * 32 / 3.29658384E+08</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="pulseBandwidth" type="xs:double">
																				<xs:annotation>
																					<xs:documentation>instrument code! Pulse bandwidth in Hz = value * 1.25E+06</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="centerFrequency" type="xs:double">
																				<xs:annotation>
																					<xs:documentation>instrument code! Center frequency in Hz = value * 1.25E+06 </xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="amplitude" type="dblPolynom"/>
																			<xs:element name="phase" type="dblPolynom"/>
																			<xs:element name="replica" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Results of chirp analysis expressed with model parameters</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="replicaQualityTooLowFlag" type="xs:boolean">
																							<xs:annotation>
																								<xs:documentation>model used instead of desired replica</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="totalPower" type="xs:float"/>
																						<xs:element name="meanAmplitude" type="xs:float"/>
																						<xs:element name="peakLocation" type="xs:double"/>
																						<xs:element name="pulseLength" type="xs:float"/>
																						<xs:element name="resolution" type="xs:float">
																							<xs:annotation>
																								<xs:documentation>3dB width</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="PSLR" type="xs:float" minOccurs="0"/>
																						<xs:element name="ISLR" type="xs:float" minOccurs="0"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																		<xs:attribute name="pulseCode" type="xs:int" use="required">
																			<xs:annotation>
																				<xs:documentation>instrument index code</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="correctedInstrumentDelay" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>corrected total time delay for the chirps used per layer </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="polLayer"/>
													<xs:element name="DRAoffset" minOccurs="0">
														<xs:annotation>
															<xs:documentation>SRA, DRAFore, DRAAft</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:NMTOKENS">
																<xs:enumeration value="SRA">
																	<xs:annotation>
																		<xs:documentation>Single Recieve Antenna (nominal)</xs:documentation>
																	</xs:annotation>
																</xs:enumeration>
																<xs:enumeration value="DRAFore">
																	<xs:annotation>
																		<xs:documentation>Dual Recieve Antenna</xs:documentation>
																	</xs:annotation>
																</xs:enumeration>
																<xs:enumeration value="DRAAft"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="totalTimeDelay" type="xs:double">
														<xs:annotation>
															<xs:documentation>[s] </xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="scanSARBeamOverlap" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="rangeTimeStart" type="xs:double">
														<xs:annotation>
															<xs:documentation>range start time of overlap to next ScanSAR beam</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="rangeTimeStop" type="xs:double"/>
													<xs:element name="farRangeBeamBorderTime" type="xs:double">
														<xs:annotation>
															<xs:documentation>derived end of this beam (midle of overlap region for SSCs). This is also the range time to which on the elevation angles and pixel coordinates in the geo grid refer to this beam and its pattern pattern.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="mergingMethod" type="string128" minOccurs="0">
														<xs:annotation>
															<xs:documentation>ScanSAR beam merging for detected products</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="lookWeight" minOccurs="0">
														<xs:annotation>
															<xs:documentation>ScanSAR subswath  look summation weighting coefficient </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:float"/>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="processingFlags">
								<xs:annotation>
									<xs:documentation>flags indicating which processing steps have been performed. Flags of steps like geocoding are given even for complex products (as "false" of course).</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="RXGainCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>receiver gain correction applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DRAChannelSyncFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>DRA channel synchronization performed</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DRAChannelDemixingPerformedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>DRA channel demixing performed</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="hybridCouplerCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>hybrid coupler correction applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="chirpDriftCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>phase and amplitude drift over time corrected</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="chirpReplicaUsedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>reconstructed chirp used</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="geometricDopplerUsedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>geometric Doppler centroid estimate has been used</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="noiseCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>noise correction applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="rangeSpreadingLossCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>range spreading loss compensation applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="scanSARBeamCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>ScanSAR specific beam corrections applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="spotLightBeamCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Spotlight specific azimuth beam corrections applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="azimuthPatternCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Stripmap azimuth antenna pattern correction applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="elevationPatternCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>antenna elevation pattern correction applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="polarisationCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>polarisation correction applied</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="detectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>detection performed</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="multiLookedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>multi-looking performed</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="propagationEffectsCorrectedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>range delay corrected</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="geocodedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>geocoding performed</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="incidenceAngleMaskGeneratedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>for EEC products</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="nominalProcessingPerformedFlag" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>nominal L1B Processing chain or (manually) configured steps</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="instrument">
					<xs:annotation>
						<xs:documentation>Sensor specific parameters at the (raw data) time of the data acquisition.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="instrumentInfoCoordinateType">
								<xs:annotation>
									<xs:documentation>raw data time tags (not zero Doppler)</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="string20">
										<xs:enumeration value="RAW"/>
										<xs:enumeration value="ZERODOPPLER"/>
										<xs:enumeration value="UNDEFINED"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="radarParameters">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="centerFrequency" type="xs:double">
											<xs:annotation>
												<xs:documentation>[Hz] (9.65 GHz)</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="settings" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>SAR Instrument Settings for different beams/polarisations</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="polLayer"/>
										<xs:element name="DRAoffset" minOccurs="0">
											<xs:annotation>
												<xs:documentation>SRA, DRAFore, DRAAft</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKENS">
													<xs:enumeration value="SRA">
														<xs:annotation>
															<xs:documentation>Single Recieve Antenna (nominal)</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="DRAFore">
														<xs:annotation>
															<xs:documentation>Dual Recieve Antenna</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="DRAAft"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="beamID" type="string20"/>
										<xs:element name="numberOfRxGainChanges" type="xs:int">
											<xs:annotation>
												<xs:documentation>during the acquisition time interval of the data processed for this product</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="rxGainSetting" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>gain setting sequences before ADC conversion</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="startTimeUTC" type="xs:dateTime"/>
													<xs:element name="stopTimeUTC" type="xs:dateTime"/>
													<xs:element name="rxGain">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:float">
																	<xs:attribute name="code"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="quantisationID" type="string20">
											<xs:annotation>
												<xs:documentation>BAQ</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="quantisationControl">
											<xs:annotation>
												<xs:documentation>compression factors (4/4)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="string20"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="rxBandwidth">
											<xs:annotation>
												<xs:documentation>RX Bandwidth  [Hz] exact value</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:double">
														<xs:attribute name="code" type="string20" use="required">
															<xs:annotation>
																<xs:documentation>instrument code</xs:documentation>
															</xs:annotation>
														</xs:attribute>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="RSF">
											<xs:annotation>
												<xs:documentation>range sampling frequency  [Hz] exact value</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:double">
														<xs:attribute name="code" type="string20" use="required">
															<xs:annotation>
																<xs:documentation>instrument code</xs:documentation>
															</xs:annotation>
														</xs:attribute>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="numberOfPRFChanges" type="xs:int">
											<xs:annotation>
												<xs:documentation>0 = PRF constant in one subswath (but different for each ScanSAR beam) of the datatake </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="numberOfEchoWindowPositionChanges" type="xs:int">
											<xs:annotation>
												<xs:documentation>0 = SWST constantly different in the SC/SL/HS beams</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="numberOfEchoWindowLengthChanges" type="xs:int">
											<xs:annotation>
												<xs:documentation>expected to remain constant</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="numberOfSettingRecords" type="xs:int">
											<xs:annotation>
												<xs:documentation>number of the following parameter blocks</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="settingRecord" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>continous data segments (for each polarisation and beam) with constant instrument settings for a given time range. If the parameters do not change between SC bursts, all bursts in one beam and one pol channel belong to one segment. Analogous for spotlight mode azimuth beams.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="dataSegment">
														<xs:annotation>
															<xs:documentation>Since they are separately annotated for each (ScanSAR- (and spotlight)) elevation beam, the blocks of beam settings may cover the time span of several (or even all) cycles. They thus have gaps in time but not in the sequence of the data for one beamfile. For uncorrupted data, they are identical for each polarisation (using the same data raster).</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="startTimeUTC" type="xs:dateTime"/>
																<xs:element name="stopTimeUTC" type="xs:dateTime"/>
																<xs:element name="numberOfRows" type="xs:int"/>
															</xs:sequence>
															<xs:attribute name="segmentID" type="xs:int" use="required"/>
														</xs:complexType>
													</xs:element>
													<xs:element name="PRF">
														<xs:annotation>
															<xs:documentation>pulse repetition frequency. The code addresses the commanded PRF from the instrument PRF table in [Hz] (exact value)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:double">
																	<xs:attribute name="code" type="string20" use="required"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="echoWindowPosition">
														<xs:annotation>
															<xs:documentation>sampling window start time (SWST)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:double">
																	<xs:attribute name="code" type="string20" use="required"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="echowindowLength">
														<xs:annotation>
															<xs:documentation>number of samples</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:double">
																	<xs:attribute name="code" type="string20" use="required"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="pulseType" type="string20">
														<xs:annotation>
															<xs:documentation>from SAR header</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="echoIndex" type="xs:int">
														<xs:annotation>
															<xs:documentation>travelling pulses (rank)</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="calibration">
					<xs:annotation>
						<xs:documentation>The base for the application of radiometric and other data corrections. Nominal performance values for quality assessment.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="calibrationData">
								<xs:annotation>
									<xs:documentation>Input parameters used for calibration of this product </xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="radiometricDEMID">
											<xs:annotation>
												<xs:documentation>DEM used for radiometric calibration (identical to default for geolocation grid unless later altered by geocoding). e.g. Globe, STRM7.5,_</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="string255"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="numberOfAntennaPatterns" type="xs:int">
											<xs:annotation>
												<xs:documentation>Antenna Patterns of each channel and beam</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="antennaPattern" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>elevation and azimuth pattern (expressed in relation to the mechanical boresight) of the beams used in this aquisition (mainly taken from IOCS Aux Product). The projection onto the image data is performed by the geolocation grid. One set of pattern for each polarization and DRA channel combination.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="polLayer"/>
													<xs:element name="DRAoffset" minOccurs="0">
														<xs:annotation>
															<xs:documentation>SRA, DRAFore, DRAAft</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:NMTOKENS">
																<xs:enumeration value="SRA">
																	<xs:annotation>
																		<xs:documentation>Single Recieve Antenna (nominal)</xs:documentation>
																	</xs:annotation>
																</xs:enumeration>
																<xs:enumeration value="DRAFore">
																	<xs:annotation>
																		<xs:documentation>Dual Recieve Antenna</xs:documentation>
																	</xs:annotation>
																</xs:enumeration>
																<xs:enumeration value="DRAAft"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="elevationPattern" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>used to correct range dependence of the gain. One patteren for each ScanSAR beam.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="referenceGain" type="xs:float">
																	<xs:annotation>
																		<xs:documentation>for elevation: [dBi]
 max. absolute 2-way gain
</xs:documentation>
																	</xs:annotation>
																</xs:element>
