{
  "dependencies": {
    "auto-changelog": "^2.4.0",
    "package.json": "^2.0.1"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "hideEmptyReleases": true,
    "ignoreCommitPattern": "^(.{0,5}|.*bump.*|ready.*|.*changelog.*|.*formatting.*|.*typo.*|chore:.*)$",
    "releaseSummary": true
  }
}
